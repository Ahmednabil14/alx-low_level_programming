#include <stdio.h>
/**
 * main - prints the number of arguments passed into it.
 * @argc: number of file running
 * @argv: array of string value of file running
 * Return: 0
 */
int main(int argc, char *argv[])
{
	(void) argv;
	int i;
	for (i = 0 ; i <= argc ; i++)
	{
		printf("%d\n", i);
	}
	return (0);
}
