#include "main.h"
/**
 * _strchr - function that locates a character in a string.
 * @s: the original string
 * @c: char you want to print from
 * Return: the new pointer of the string
 */
char *_strchr(char *s, char c)
{
	char **new = &s;

	while (*s != c)
	{
		*new = '\0';
		s++;
	}
	return (*new);
}
